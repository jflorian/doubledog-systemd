[Login]
<% if defined?(@n_auto_vts) -%>
NAutoVTs=<%= @n_auto_vts %>
<% end -%>
<% if defined?(@reserve_vt) -%>
ReserveVT=<%= @reserve_vt %>
<% end -%>
<% if defined?(@kill_user_processes) -%>
KillUserProcesses=<%= @kill_user_processes %>
<% end -%>
<% if defined?(@kill_only_users) -%>
KillOnlyUsers=<%= @kill_only_users.join(' ') %>
<% end -%>
<% if defined?(@kill_exclude_users) -%>
KillExcludeUsers=<%= @kill_exclude_users.join(' ') %>
<% end -%>
<% if defined?(@inhibit_delay_max_sec) -%>
InhibitDelayMaxSec=<%= @inhibit_delay_max_sec %>
<% end -%>
<% if defined?(@handle_power_key) -%>
HandlePowerKey=<%= @handle_power_key %>
<% end -%>
<% if defined?(@handle_suspend_key) -%>
HandleSuspendKey=<%= @handle_suspend_key %>
<% end -%>
<% if defined?(@handle_hibernate_key) -%>
HandleHibernateKey=<%= @handle_hibernate_key %>
<% end -%>
<% if defined?(@handle_lid_switch) -%>
HandleLidSwitch=<%= @handle_lid_switch %>
<% end -%>
<% if defined?(@handle_lid_switch_docked) -%>
HandleLidSwitchDocked=<%= @handle_lid_switch_docked %>
<% end -%>
<% if defined?(@power_key_ignore_inhibited) -%>
PowerKeyIgnoreInhibited=<%= @power_key_ignore_inhibited %>
<% end -%>
<% if defined?(@suspend_key_ignore_inhibited) -%>
SuspendKeyIgnoreInhibited=<%= @suspend_key_ignore_inhibited %>
<% end -%>
<% if defined?(@hibernate_key_ignore_inhibited) -%>
HibernateKeyIgnoreInhibited=<%= @hibernate_key_ignore_inhibited %>
<% end -%>
<% if defined?(@lid_switch_ignore_inhibited) -%>
LidSwitchIgnoreInhibited=<%= @lid_switch_ignore_inhibited %>
<% end -%>
<% if defined?(@holdoff_timeout_sec) -%>
HoldoffTimeoutSec=<%= @holdoff_timeout_sec %>
<% end -%>
<% if defined?(@idle_action) -%>
IdleAction=<%= @idle_action %>
<% end -%>
<% if defined?(@idle_action_sec) -%>
IdleActionSec=<%= @idle_action_sec %>
<% end -%>
<% if defined?(@runtime_directory_size) -%>
RuntimeDirectorySize=<%= @runtime_directory_size %>
<% end -%>
<% if defined?(@remove_ipc) -%>
RemoveIPC=<%= @remove_ipc %>
<% end -%>
