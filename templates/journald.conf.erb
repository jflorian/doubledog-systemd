[Journal]
<% if defined?(@storage) -%>
Storage=<%= @storage %>
<% end -%>
<% if defined?(@compress) -%>
Compress=<%= @compress %>
<% end -%>
<% if defined?(@seal) -%>
Seal=<%= @seal %>
<% end -%>
<% if defined?(@split_mode) -%>
SplitMode=<%= @split_mode %>
<% end -%>
<% if defined?(@sync_interval_sec) -%>
SyncIntervalSec=<%= @sync_interval_sec %>
<% end -%>
<% if defined?(@rate_limit_interval_sec) -%>
RateLimitIntervalSec=<%= @rate_limit_interval_sec %>
<% end -%>
<% if defined?(@rate_limit_burst) -%>
RateLimitBurst=<%= @rate_limit_burst %>
<% end -%>
<% if defined?(@system_max_use) -%>
SystemMaxUse=<%= @system_max_use %>
<% end -%>
<% if defined?(@system_keep_free) -%>
SystemKeepFree=<%= @system_keep_free %>
<% end -%>
<% if defined?(@system_max_file_size) -%>
SystemMaxFileSize=<%= @system_max_file_size %>
<% end -%>
<% if defined?(@system_max_files) -%>
SystemMaxFiles=<%= @system_max_files %>
<% end -%>
<% if defined?(@runtime_max_use) -%>
RuntimeMaxUse=<%= @runtime_max_use %>
<% end -%>
<% if defined?(@runtime_keep_free) -%>
RuntimeKeepFree=<%= @runtime_keep_free %>
<% end -%>
<% if defined?(@runtime_max_file_size) -%>
RuntimeMaxFileSize=<%= @runtime_max_file_size %>
<% end -%>
<% if defined?(@runtime_max_files) -%>
RuntimeMaxFiles=<%= @runtime_max_files %>
<% end -%>
<% if defined?(@max_retention_sec) -%>
MaxRetentionSec=<%= @max_retention_sec %>
<% end -%>
<% if defined?(@max_file_sec) -%>
MaxFileSec=<%= @max_file_sec %>
<% end -%>
<% if defined?(@forward_to_syslog) -%>
ForwardToSyslog=<%= @forward_to_syslog %>
<% end -%>
<% if defined?(@forward_to_kmsg) -%>
ForwardToKMsg=<%= @forward_to_kmsg %>
<% end -%>
<% if defined?(@forward_to_console) -%>
ForwardToConsole=<%= @forward_to_console %>
<% end -%>
<% if defined?(@forward_to_wall) -%>
ForwardToWall=<%= @forward_to_wall %>
<% end -%>
<% if defined?(@tty_path) -%>
TTYPath=<%= @tty_path %>
<% end -%>
<% if defined?(@max_level_store) -%>
MaxLevelStore=<%= @max_level_store %>
<% end -%>
<% if defined?(@max_level_syslog) -%>
MaxLevelSyslog=<%= @max_level_syslog %>
<% end -%>
<% if defined?(@max_level_kmsg) -%>
MaxLevelKMsg=<%= @max_level_kmsg %>
<% end -%>
<% if defined?(@max_level_console) -%>
MaxLevelConsole=<%= @max_level_console %>
<% end -%>
<% if defined?(@max_level_wall) -%>
MaxLevelWall=<%= @max_level_wall %>
<% end -%>
